        "gulp-sourcemaps": "^3.0.0",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.21",
        "node-sass": "^7.0.3",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.54.9"
    },
    "dependencies": {
        "browser-sync": "^2.27.10",
        "gulp": "^4.0.2",
        "gulp-htmlmin": "^5.0.1",
        "gulp-imagemin": "^8.0.0",
        "gulp-pug": "^5.0.0",
        "gulp-w3c-html-validator": "^5.1.3"
    }
